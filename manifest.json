{
  "name": "QuickActions Chrome",
  "version": "0.0.1",
  "description": "Puts the internet at your fingertips",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["app-ep.css", "css/style.css"],
      "js": ["scriptTagContext.js", "js/lib/require.js", "app-cs.js"]
    }
  ],
  "permissions": [
    "http://*/",
    "https://*/",
    "tabs",
    "bookmarks",
    "history",
    "topSites",
    "identity",
    "alarms",
    "notifications",
    "downloads",
    "downloads.open"
  ],
  "web_accessible_resources": [
    "js/*",
    "css/*",
    "extension-lib/*",
    "providers/*"
 ],
 "key": "",
 "oauth2": {
    "client_id": "351650823507-cplh77tatfeam3db2ugie8j1qtbsa6ht.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.compose",
      "https://www.googleapis.com/auth/contacts.readonly"
    ]
  },
 "background": {
   "scripts": ["app-ep.js"],
   "persistent": false
  },
  "commands": {
        "toggle-open": {
          "suggested_key": {
            "default": "Ctrl+M",
            "mac": "Command+M"
          },
          "description": "Toggle QuickActions"
        }
  },
  "manifest_version": 2
}
